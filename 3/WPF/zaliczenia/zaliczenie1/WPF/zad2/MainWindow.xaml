<Window x:Class="zad2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zad2"
        mc:Ignorable="d"
        Title="Wykształcenie" Height="200" Width="280">
    <StackPanel Orientation="Vertical">
        <RadioButton Content="Podstawowe" Margin="3 5 0 5" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
        <RadioButton Content="Niepełne średnie" Margin="3 0 0 10" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
        <RadioButton Content="Średnie" Margin="3 0 0 10" IsChecked="True" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
        <RadioButton Content="Niepełne wyższe" Margin="3 0 0 10" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
        <RadioButton Content="Wyższe" Margin="3 0 0 10" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
        <RadioButton Content="Brak danych" Margin="3 0 0 10" FontFamily="Verdana" FontSize="14" Foreground="Navy"/>
    </StackPanel>
</Window>
